{
  "framework": "vite",
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors 'self' YOUR_WORDPRESS_DOMAIN.com;"
        }
      ]
    }
  ]
} 